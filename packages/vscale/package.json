    "@internal/ts-config": "workspace:*",
    "@rushstack/eslint-patch": "~1.1.4",
    "eslint": "~8.18.0",
    "vite": "3.2.6",
    "typescript": "4.9.5",
    "@internal/jest-config": "workspace:*",
    "jest": "~29.5.0",
    "@jest/globals": "~29.5.0",
    "ts-jest": "~29.1.0",
    "@types/jest": "~29.5.0",
    "@types/node": "*"
