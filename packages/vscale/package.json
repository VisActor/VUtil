{
  "name": "@visactor/vscale",
  "version": "0.19.4-alpha.0",
  "description": "Scales for visual encoding, used in VGrammar, VTable",
  "keywords": [
    "scale",
    "visualization",
    "VGrammar",
    "storytelling",
    "VisActor"
  ],
  "author": {
